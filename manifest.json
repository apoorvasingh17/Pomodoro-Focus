{
 "name": "Pomodoro Focus",
 "version": "1.0",
 "manifest_version": 2,
 "description": "Getting distracted by certain websites? Use Pomodoro Focus",
 "background_page" : "background.html",
 "options_page": "options.html",
 "icons": {
 "16" : "16.png",
 "48" : "48.png",
 "128" : "128.png"
 },
 "browser_action": {
 "default_icon": "16.png",
 "default_title": "Pomodoro Focus",
 "default_popup": "popup.html"
 },
 "permissions": ["tabs"]
}
